{
  "COMPONENTS_JSON_FILE": "install/components.json",
  "CREDENTIALS_FILE": ".nebulaiq/credentials.toml",
  "BASE_CONFIG_FILE": "base_install_config.json",
  "PROJECT_CONFIG_SUFFIX": "_install_config.json",
  "DEFAULT_NAMESPACE": "nebulaiq-telemetry",
  "DEFAULT_DOCKER_REGISTRY": "nebulaiqdemo.azurecr.io",
