  "DOCKER_PASSWORD_ENV": "DOCKER_PASSWORD",
  "DOCKER_REGISTRY_ENV": "DOCKER_REGISTRY",
  "NEBULAIQ_NAMESPACE_ENV": "NEBULAIQ_NAMESPACE",
  "SECRET_NAME_ENV": "SECRET_NAME"
